package manuale_di_java_8_se_cdsc.modulo_4_gestione_flusso;

public class E_4b {

	public static void main(String[] args) 
	{

		for(int i=0; i < 126; i++)
		{
			//Partendo da a, somma l'indice che punta al carattere successivo e fa il casting (char) per stampare il carattere.
			System.out.println(((char) ('a'+i)));
		}

	}

}
