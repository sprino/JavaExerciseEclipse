package manuale_di_java_8_se_cdsc.modulo_2_componenti_fondamentali;

/** Exe 2d_classe */
public class E_2d_Quadrato
{	
	/*Dichiaro attribbuto*/
	public double lato;
	
	/* Metodo che calcola il perimetro.*/
	public double perimetro()
	{
		return (lato*4);
	}
	
	/*Metodo che calcola l'area.*/
	public double area()
	{
		return (lato*lato);
	}
}
